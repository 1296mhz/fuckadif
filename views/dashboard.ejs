<% include partials/head-dashboard %>

<body class="theme-red">
  <!-- Page Loader -->
  <%- include('components/pageLoader', {}); %>
  <!-- #END# Page Loader -->
  <!-- Overlay For Sidebars -->
  <%- include('components/overlayForSideBars', {}); %>
  <!-- #END# Overlay For Sidebars -->
  <!-- Search Bar -->
  <%- include('components/searchBar', {}); %>
  <!-- #END# Search Bar -->
  <!-- Top Bar -->
  <%- include('components/topBar', { title: title }); %>
  <!-- #Top Bar -->
  <section>
    <!-- Left Sidebar -->
    <%- include('components/leftSideBar', {
        menu: menu
   }); %>
    <!-- #END# Left Sidebar -->
    <!-- Right Sidebar -->
    <%- include('components/rightSideBar', {}); %>
    <!-- #END# Right Sidebar -->
  </section>

  <section class="content">
    <div class="container-fluid">
      <!-- Block Header -->
      <%- include('components/blockHeader', {
          viewName: title
        }); %>
      <!-- #END# Block Header -->

      <div class="row clearfix">


        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
          <div class="row">
            <div class="card">
              <div class="header">
                <h2>
                  Журналы

                </h2>
                <ul class="header-dropdown m-r--5">
                  <li class="dropdown">
                    <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button"
                      aria-haspopup="true" aria-expanded="false">
                      <i class="material-icons">more_vert</i>
                    </a>
                    <ul class="dropdown-menu pull-right">
                      <li><a href="javascript:void(0);">Action</a></li>
                      <li><a href="javascript:void(0);">Another action</a></li>
                      <li><a href="javascript:void(0);">Something else here</a></li>
                    </ul>
                  </li>
                </ul>
              </div>
              <div class="body">
                <div class="row clearfix">
                  <div class="col-md-6">
                    <select class="form-control show-tick callsign">
                      <% Object.keys(callsigns).forEach(function(callsign){ %>
                      <option id="_<%= callsign %>"> <%= callsign %></option>
                      <%  }); %>

                    </select>
                  </div>
                  <div class="col-md-6">
                    <select class="form-control show-tick locators">
                    </select>
                  </div>
                </div>
              </div>
            </div>

          </div>
          <div class="row">
            <div class="row qthloc-widgets">
              <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                <div class="info-box bg-pink hover-expand-effect">
                  <div class="icon">
                    <i class="material-icons">map</i>
                  </div>
                  <div class="content">
                    <div class="text">Всего Секторов</div>
                    <div class="number count-to sectors-locator" data-from="0" data-to="0" data-speed="1"
                      data-fresh-interval="20"></div>
                  </div>
                </div>
              </div>
              <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                <div class="info-box bg-light-green hover-expand-effect">
                  <div class="icon">
                    <i class="material-icons">view_module</i>
                  </div>
                  <div class="content">
                    <div class="text">Всего квадратов</div>
                    <div class="number count-to big-locator" data-from="0" data-to="0" data-speed="1"
                      data-fresh-interval="20"></div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="bands-tables"></div>
          </div>
        </div>

        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
          <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
              <!-- MAP  -->
              <%- include('components/lmap', {} ); %>
              <!-- #END# MAP -->
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <span id="callsigns" hidden>
    <%= JSON.stringify(callsigns); %>
  </span>
  <span id="locator-to-lat-lng-convert" hidden>
    <%= JSON.stringify(locatorToLatLngConvert); %>
  </span>
  <!-- Libs Js -->
  <%- include('libs/dashboard', {}); %>
  <!-- #END# Libs Js -->
</body>

</html>